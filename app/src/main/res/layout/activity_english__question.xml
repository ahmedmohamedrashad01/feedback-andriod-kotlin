<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:alpha="0.8"
    android:background="@drawable/thirdenglish"
    android:fillViewport="true"
    tools:context=".MainActivity">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:alpha="0.3"
            android:scaleType="centerCrop"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="646dp"
            android:layout_height="70dp"
            android:layout_marginTop="16dp"
            android:fontFamily="sans-serif-light"
            android:paddingStart="8dp"
            android:paddingTop="5dp"
            android:paddingRight="10sp"
            android:shadowColor="#155FCF"
            android:text="1- Your Overall Satisfaction"
            android:textColor="#B0041F65"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.029"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="646dp"
            android:layout_height="70dp"
            android:layout_marginTop="12dp"
            android:paddingStart="8dp"
            android:paddingTop="5dp"
            android:text="5- Price"
            android:textColor="#B0041F65"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.029"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout5" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="646dp"
            android:layout_height="70dp"
            android:layout_marginTop="32dp"
            android:paddingStart="8dp"
            android:paddingTop="5dp"
            android:text="4- Staff Performance"
            android:textColor="#B0041F65"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.029"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout3" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="646dp"
            android:layout_height="70dp"
            android:layout_marginTop="12dp"
            android:paddingStart="8dp"
            android:paddingTop="5dp"
            android:text="3- Temperature"
            android:textColor="#B0041F65"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.022"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout2" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="646dp"
            android:layout_height="70dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="15dp"
            android:paddingStart="8dp"
            android:paddingTop="5dp"
            android:paddingRight="10dp"
            android:text="2- Quietness"
            android:textColor="#B0041F65"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.024"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout" />

        <LinearLayout
            android:id="@+id/linearLayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="15dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="15dp"
            android:layout_weight="5"
            android:elevation="50dp"
            android:orientation="horizontal"
            android:paddingStart="10dp"
            android:paddingEnd="10dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView">

            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/imgFirstMada"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"

                android:onClick="checkFirstRow"
                android:src="@drawable/firstfeed"
                app:civ_border_color="#FF000000"
                app:civ_border_width="2dp"
                tools:ignore="OnClick" />

            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/imgSecondMada"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:onClick="checkFirstRow"
                android:src="@drawable/secondfeed"
                app:civ_border_color="#FF000000"
                app:civ_border_width="2dp"
                tools:ignore="OnClick" />

            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/imgThirdMada"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:src="@drawable/thirdfeed"
                android:onClick="checkFirstRow"
                app:civ_border_color="#FF000000"
                app:civ_border_width="2dp"
                tools:ignore="OnClick" />

            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/imgFourthMada"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:src="@drawable/fifthfeed"
                android:onClick="checkFirstRow"
                app:civ_border_color="#FF000000"
                app:civ_border_width="2dp"
                tools:ignore="OnClick" />

            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/imgFifthMada"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:src="@drawable/fourthfeed"
                android:onClick="checkFirstRow"
                app:civ_border_color="#FF000000"
                app:civ_border_width="2dp"
                tools:ignore="OnClick" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearLayout2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="15dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="15dp"
            android:orientation="horizontal"
            android:paddingStart="10dp"
            android:paddingEnd="10dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView2">

            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/imgFirstdHodoo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:onClick="checkSecondRow"
                android:src="@drawable/firstfeed"
                app:civ_border_color="#FF000000"
                app:civ_border_width="2dp"
                tools:ignore="OnClick" />

            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/imgSecondHodoo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:onClick="checkSecondRow"
                android:src="@drawable/secondfeed"
                app:civ_border_color="#FF000000"
                app:civ_border_width="2dp"
                tools:ignore="OnClick" />

            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/imgThirdHodoo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:onClick="checkSecondRow"
                android:src="@drawable/thirdfeed"
                app:civ_border_color="#FF000000"
                app:civ_border_width="2dp"
                tools:ignore="OnClick" />

            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/imgFourthHodoo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:onClick="checkSecondRow"
                android:src="@drawable/fifthfeed"
                app:civ_border_color="#FF000000"
                app:civ_border_width="2dp"
                tools:ignore="OnClick" />

            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/imgFifthHodoo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:onClick="checkSecondRow"
                android:src="@drawable/fourthfeed"
                app:civ_border_color="#FF000000"
                app:civ_border_width="2dp"
                tools:ignore="OnClick" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearLayout3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="15dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="15dp"
            android:orientation="horizontal"
            android:paddingStart="10dp"
            android:paddingEnd="10dp"
            android:weightSum="5"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView4">

            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/imgFirstHarara"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:onClick="checkThirddRow"
                android:src="@drawable/firstfeed"
                app:civ_border_color="#FF000000"
                app:civ_border_width="2dp"
                tools:ignore="OnClick" />

            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/imgSecondHarara"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:onClick="checkThirddRow"
                android:src="@drawable/secondfeed"
                app:civ_border_color="#FF000000"
                app:civ_border_width="2dp"
                tools:ignore="OnClick" />

            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/imgThirdHarara"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:onClick="checkThirddRow"
                android:src="@drawable/thirdfeed"
                app:civ_border_color="#FF000000"
                app:civ_border_width="2dp"
                tools:ignore="OnClick" />

            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/imgFourthHarara"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:onClick="checkThirddRow"
                android:src="@drawable/fifthfeed"
                app:civ_border_color="#FF000000"
                app:civ_border_width="2dp"
                tools:ignore="OnClick" />

            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/imgFifthHarara"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:onClick="checkThirddRow"
                android:src="@drawable/fourthfeed"
                app:civ_border_color="#FF000000"
                app:civ_border_width="2dp"
                tools:ignore="OnClick" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearLayout5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:orientation="horizontal"
            android:paddingStart="10dp"
            android:paddingEnd="10dp"
            android:weightSum="5"
            app:layout_constraintTop_toBottomOf="@+id/textView5">

            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/imgFirstAdaa"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:onClick="checkFourthdRow"
                android:src="@drawable/firstfeed"
                app:civ_border_color="#FF000000"
                app:civ_border_width="2dp"
                tools:ignore="OnClick" />

            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/imgSecondAdaa"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:onClick="checkFourthdRow"
                android:src="@drawable/secondfeed"
                app:civ_border_color="#FF000000"
                app:civ_border_width="2dp"
                tools:ignore="OnClick" />

            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/imgThirdAdaa"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:onClick="checkFourthdRow"
                android:src="@drawable/thirdfeed"
                app:civ_border_color="#FF000000"
                app:civ_border_width="2dp"
                tools:ignore="OnClick" />

            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/imgFourthAdaa"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:onClick="checkFourthdRow"
                android:src="@drawable/fifthfeed"
                app:civ_border_color="#FF000000"
                app:civ_border_width="2dp"
                tools:ignore="OnClick" />

            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/imgFifthAdaa"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:onClick="checkFourthdRow"
                android:src="@drawable/fourthfeed"
                app:civ_border_color="#FF000000"
                app:civ_border_width="2dp"
                tools:ignore="OnClick" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearLayout6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="15dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="15dp"
            android:orientation="horizontal"
            android:paddingStart="10dp"
            android:paddingEnd="10dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView3">

            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/imgFirstPrice"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:onClick="checkFifthdRow"
                android:src="@drawable/firstfeed"
                app:civ_border_color="#FF000000"
                app:civ_border_width="2dp"
                tools:ignore="OnClick" />

            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/imgSecondPrice"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:onClick="checkFifthdRow"
                android:src="@drawable/secondfeed"
                app:civ_border_color="#FF000000"
                app:civ_border_width="2dp"
                tools:ignore="OnClick" />

            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/imgThirdPrice"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:onClick="checkFifthdRow"
                android:src="@drawable/thirdfeed"
                app:civ_border_color="#FF000000"
                app:civ_border_width="2dp"
                tools:ignore="OnClick" />

            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/imgFourthPrice"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:onClick="checkFifthdRow"
                android:src="@drawable/fifthfeed"
                app:civ_border_color="#FF000000"
                app:civ_border_width="2dp"
                tools:ignore="OnClick" />

            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/imgFifthPrice"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:onClick="checkFifthdRow"
                android:src="@drawable/fourthfeed"
                app:civ_border_color="#FF000000"
                app:civ_border_width="2dp"
                tools:ignore="OnClick" />
        </LinearLayout>


        <Button
            android:id="@+id/btnSend"
            android:layout_width="510dp"
            android:layout_height="97dp"
            android:layout_marginTop="48dp"
            android:layout_marginBottom="15dp"
            android:background="@drawable/circle_button"
            android:text="Send Feedback"
            android:textAllCaps="false"
            android:textColor="#ffffff"
            android:textSize="50sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.506"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout6"
            app:layout_constraintVertical_bias="0.0" />


    </androidx.constraintlayout.widget.ConstraintLayout>


</ScrollView>